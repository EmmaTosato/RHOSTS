    "\n",
    "def parse_output_file(path):\n",
    "    rows = []\n",
    "    bad = []\n",
    "    with open(path, \"r\") as f:\n",
    "        for ln, line in enumerate(f, start=1):\n",
    "            s = line.strip()\n",
    "            if not s:\n",
    "                continue  # skip empty lines\n",
    "            parts = s.split()\n",
    "            try:\n",
    "                t = int(parts[0])\n",
    "                vals = list(map(float, parts[1:]))\n",
    "                rows.append((t, vals, s))\n",
    "            except Exception:\n",
    "                bad.append((ln, s))\n",
    "    return rows, bad\n",
    "\n",
    "def list_hdf5_timepoints(hd5_path):\n",
    "    if not os.path.isfile(hd5_path):\n",
    "        raise FileNotFoundError(f\"File HD5 non trovato: {hd5_path}\")\n",
    "    tps = set()\n",
    "    with h5py.File(hd5_path, \"r\") as f:\n",
    "        def visitor(name, obj):\n",
    "            if isinstance(obj, h5py.Dataset):\n",
    "                token = name.rsplit(\"/\", 1)[-1]\n",
    "                match = re.search(r\"(\\d+)$\", token)\n",
    "                if match:\n",
    "                    tps.add(int(match.group(1)))\n",
    "        f.visititems(visitor)\n",
    "    return sorted(tps)\n",
    "\n",
    "def report_count(label, count, expected):\n",
    "    status = \"[OK]\" if count == expected else \"[ATTENZIONE]\"\n",
    "    print(f\"{status} {label}: trovati {count} elementi (attesi {expected}).\")\n",
    "    return count == expected\n",
    "\n",
    "def report_missing(reference, candidate, label):\n",
    "    missing = sorted(set(reference) - set(candidate))\n",
    "    if missing:\n",
    "        print(f\"[ATTENZIONE] Time points mancanti in {label}: {missing}\")\n",
    "    else:\n",
    "        print(f\"Nessun time point mancante in {label} rispetto al file di testo.\")\n",
    "    return missing\n",
    "\n",
    "rows, bad = parse_output_file(sub1_output)\n",
